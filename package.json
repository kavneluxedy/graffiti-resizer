{
  "name": "graffiti-resize",
  "author": "Quentin Cavenel",
  "description": "Grattifi-Resize | For Graffiti Agency's Clients and others",
  "version": "1.0.0",
  "private": false,
  "main": "public/electron-main.js",
  "homepage": "https://github.com/kavneluxedy/graffiti-resizer",
  "scripts": {
    "start": "npm run pack && electron ./dist/electron-main.js",
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.production.js && electron --noDevServer .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pack": "cross-env NODE_ENV=production webpack --config webpack.production.js --mode production",
    "dist": "npm run pack && electron-builder",
    "dist:dir": "npm run dist -- --dir -c.compression=store -c.mac.identity=null -c.extraMetadata.main=./build/electron-main.js"
  },
  "dependencies": {
    "downloadjs": "^1.4.7",
    "react": "^18.0.0",
    "react-bootstrap": "^2.2.3",
    "react-dom": "^18.0.0",
    "react-image-file-resizer": "^0.4.7",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "electron-is-dev": "^2.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
