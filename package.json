{
	"name": "graffiti-resize",
	"author": "Quentin Cavenel",
	"description": "For Graffiti Agency's Consumers and others",
	"version": "1.0.0",
	"private": false,
	"license": "UNLICENSED",
	"scripts": {
		"start": "concurrently -k \"cross-env BROWSER=none react-scripts start\" \"build-electron -d\" \"wait-on public/.build-electron-done http://localhost:3000 && electron .\"",
		"build": "build-electron && react-scripts build",
		"postinstall": "electron-builder install-app-deps",
		"dev": "electron-webpack",
		"compile": "electron-webpack src/main/index.js -o dist/bundle.js",
		"dist": "concurrently \"npm run compile\" && \"electron-builder\"",
		"dist:dir": "npm run dist --dir -c.compression=store -c.mac.identity=null"
	},
	"dependencies": {
		"downloadjs": "^1.4.7",
		"electron-webpack": "^2.8.2",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-image-file-resizer": "^0.4.7",
		"react-router-dom": "^6.3.0",
		"source-map-support": "^0.5.16"
	},
	"devDependencies": {
		"@babel/preset-react": "^7.17.12",
		"@types/react": "^18.0.9",
		"babel-loader": "^8.2.5",
		"build-electron": "^1.0.4",
		"concurrently": "^7.2.0",
		"cross-env": "^7.0.3",
		"electron": "^19.0.1",
		"electron-builder": "^23.0.3",
		"electron-is-dev": "^2.0.0",
		"react-scripts": "^5.0.1",
		"wait-on": "^6.0.1",
		"webpack": "^4.46.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"main": "public/main.js",
	"build": {
		"extraMetadata": {
			"main": "build/main.js"
		},
		"files": [
			"build/**/*"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kavneluxedy/graffiti-resizer.git"
	},
	"bugs": {
		"url": "https://github.com/kavneluxedy/graffiti-resizer/issues"
	}
}
